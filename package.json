{
  "name": "@goldy/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "test": "jest --config=jest.config.mjs",
    "test:watch": "jest --config=jest.config.mjs --watch",
    "test:coverage": "jest --config=jest.config.mjs --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/devkit": "21.2.3",
    "@nx/js": "21.2.3",
    "@nx/nest": "^21.2.3",
    "@nx/next": "^21.2.3",
    "@nx/node": "^21.2.3",
    "@nx/playwright": "21.2.3",
    "@nx/web": "21.2.3",
    "@nx/webpack": "21.2.3",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^30.0.0",
    "@types/node": "~18.16.9",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/sprintf-js": "^1.1.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "nx": "21.2.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.4.0",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "libs/shared/*"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ],
    "package.json": [
      "npx nx sync:check"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "hi-base32": "^0.5.1",
    "next": "~15.2.4",
    "puppeteer": "^24.12.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "sprintf-js": "^1.1.3",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.25",
    "uuid": "^11.1.0"
  }
}
